{
  "name": "@tokenomy/nodelib",
  "version": "1.1.0",
  "description": "NodeJS libraries for internal use",
  "scripts": {
    "build": "tsc -p .",
    "test": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn build",
    "prepublishOnly": "yarn test && yarn lint",
    "preversion": "yarn lint",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "author": "tokenomy",
  "license": "UNLICENSED",
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.5.5"
  }
}
